<script type="text/javascript">
$(document).ready(function(){

});
// 임의의 1~6
// R= Math.random()   0.0 <= R < 1.0
// *n : 가지수
// +k : 시작 숫자..
// parseInt : 정수로 변환
// 1에서부터 6까지   n:6,  k:1
// paseInt(R*6)+1;
// 화면이 호출될 때마다, 임의의 값을 할당 처리.
var num01 = parseInt(Math.random()*6)+1;
var num02 = parseInt(Math.random()*6)+1;
var sum=num01+num02;
alert(num01+"+"+num02+"="+sum);
var isOver10=confirm("합산이 10이상?");
// 확인을 누르고, 실제값이 10이상이면..
// Win 
// isOver10이 true이고, sum>=10 이면 조건 처리
if( isOver10 && sum>=10 ){
	alert("Win!!");
}else{
	alert("Lost!!");
}




