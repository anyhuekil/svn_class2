package springweb.z01_dto;

public class Calculator {
	private int num01;
	private int num02;
	private int calIdx;
	private int result;
	public int getNum01() {
		return num01;
	}
	public void setNum01(int num01) {
		this.num01 = num01;
	}
	public int getNum02() {
		return num02;
	}
	public void setNum02(int num02) {
		this.num02 = num02;
	}
	public int getCalIdx() {
		return calIdx;
	}
	public void setCalIdx(int calIdx) {
		this.calIdx = calIdx;
	}
	public int getResult() {
		return result;
	}
	public void setResult(int result) {
		this.result = result;
	}
	
}
